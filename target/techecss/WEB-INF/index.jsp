	crossorigin="anonymous">

<title>Hello, world!</title>
</head>

<frameset rows="18%,78%" border="1">
	<frame src="header" name="header" />
	<frameset cols="15%,80%" framespacing="1px" frameborder="no">
		<frame src="courses" name="course" />
		<frameset rows="80%,25%*" border="1">
			<frame src="main" name="main" />
			<frame src="footer" name="footer" scrolling="no" />
			<noframes>
				<body>The browser you are working does not support frames.
				</body>
			</noframes>
		</frameset>
	</frameset>



	<!-- Optional JavaScript -->
	<!-- jQuery first, then Popper.js, then Bootstrap JS -->
